{
  "name": "remove-function-analyzer-vscode",
  "displayName": "Remove Function Analyzer",
  "description": "VS Code extension to remove functions by right-clicking on their names",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:removeFunctionAnalyzer.removeFunction"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "removeFunctionAnalyzer.removeFunction",
        "title": "Remove Function"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "removeFunctionAnalyzer.removeFunction",
          "when": "editorHasSelection && editorLangId == csharp",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.70.0",
    "typescript": "^4.7.4",
    "vscode-test": "^1.6.2"
  }
}